<% if any_user_logged_in? %>
  <h1>Welcome, <%= current_user %></h1>
<% end %>

<h1>Links</h1>

<ul id="links">
  <% @links.each do |link| %>
    <li>
      Title: <%= link.title %>
      URL: <%= link.url   %>
      Tags: <%link.tags.each do |tag| %>
        <%= tag.name %>
      <% end %>
    </li>
  <% end %>
</ul>
